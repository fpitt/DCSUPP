filterFunction = ($sce)->
    return (val)->
        return $sce.trustAsHtml(val)

###
ProcessHTML = () ->
	output = {}

	output.bootstrapAngular = ->
		$('[ng-app]').each ->
		module = $(this).attr('ng-app')
		angular.bootstrap(this, [module])

	return output
###


angular
  .module("dcsupp", ['ui.router'])
  .filter('html', filterFunction)
  #.service("processHtml", ProcessHTML)